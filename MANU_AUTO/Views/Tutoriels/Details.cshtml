@model MANU_AUTO.Models.Tutoriel

@{
    ViewData["Title"] = "Details";
}

<h3>Détails du tutoriel @Model.Titre</h3>

<div>
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Titre)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Titre)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Dcc)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Dcc)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Contenu)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Contenu)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.VideoLink)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.VideoLink)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Dml)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Dml)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id" class="btn btn-primary">Modifier</a> 
    <a asp-action="Index">Liste des tutoriels</a>
</div>
